@model IEnumerable<I_FOX_V1.Models.Resumo>

@section Estilos{
    <link rel="stylesheet" href="~/css/resumoFotos.css" />
}

<section class="funcao">

    <div class="bloco">
        <form method="post" enctype="multipart/form-data">

            <button class="botao">Deletar Arquivo</button>

            <div class="imagens_resumo">

                <div class="elemento_imagem">
                    <img src="~/img/tela_inicial/foto1.png" />
                    <label>Selecione</label>
                    <input type="checkbox"/>
                </div>
            </div>

            <div class="file">
                <input name="arquivo" type="file" id="file">
                <button class="botao1">Salvar Arquivo</button>
            </div>

            <br><br>
        </form>

    </div>
</section>
